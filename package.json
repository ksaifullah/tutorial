{
  "name": "tutorial",
  "version": "1.0.0",
  "description": "typescript tutorial",
  "main": "dist/index.js",
  "scripts": {
    "compile": "rm -rf dist && tsc -p tsconfig.app.json",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet",
    "test": "jest --forceExit --detectOpenHandles",
    "watch": "concurrently -s last -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"yellow.bold,green.bold\" \"tsc -w -p tsconfig.app.json\" \"nodemon dist/server.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksaifullah/tutorial.git"
  },
  "keywords": [
    "tutorial",
    "typescript"
  ],
  "author": "khalid@outlook.com.au",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ksaifullah/tutorial/issues"
  },
  "homepage": "https://github.com/ksaifullah/tutorial#readme",
  "devDependencies": {
    "@types/eslint": "^7.2.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.39",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "concurrently": "^6.0.2",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
